    "build:less:constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "build:less:icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "build:less:variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "build:webpack": "cross-env NODE_ENV=production webpack --progress --colors --config ./.config/webpack.config.js",
    "clean": "rimraf ./dist",
    "copy": "run-p copy:*",
    "copy:root": "cpx \"./dist/**/*\" \"../../../dist\"",
    "copy:lib": "cpx \"./dist/**/*\" \"../../libs/ui-kit/dist\"",
    "version": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
